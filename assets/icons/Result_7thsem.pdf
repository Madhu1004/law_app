<script type="text/javascript">
// var updateIntervalId;

function generateLoginQr(){
	var second=30;
	var x;
var payload=( window.location.origin+'~~~'+$('#userName').val())
var body=''

	if($('#hdnUserRole').val().toLowerCase().includes("student")){
		identifier = "student-login-general"
	}else{
		identifier='employee-login-general'
	}
	$.ajax({
		url:'generateGeneralQr.json',
		data:{
			expiration:"30",
			payload:payload,
			identifier:identifier
		},
		dataType:'text',
		type:'POST',
		beforeSend:function() {
		},
		complete: function() {
		},
		success:function(str){
		body+=`
		<img style="flex:0 1 auto" align="middle" src="https://chart.googleapis.com/chart?chs=500x400&cht=qr&chl=`;
		body+= str;
		body += `&choe=UTF-8">`;
		let footer='<div style="float:left">Refreshing in: <i id="timer"></<i></div><a type="button" style="float:right" onclick="changeQrLoginFlag(false);clearInterval('+x+');" data-dismiss="modal" aria-hidden="true" class="btn btn-mini btn-danger">Close</a>';
		$('#QrgenerationBody').html(body);
		$('#QrgenerationFooter').html(footer);
		$('#QrgenerationModal').modal('show');
		}
	});
//	updateIntervalId = setInterval(generateLoginQr, 30000);
	x = setInterval(function() {
		second=second-1;
		document.getElementById("timer").innerHTML = second + "s ";
	       if(second<=0){
	    	   second=30;
	    	   generateLoginQr();
	    	   clearInterval(x);
	       }
	}, 1000);
	}

function changeQrLoginFlag(isQrLoginEnabled){

